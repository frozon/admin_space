<h1><%= @resource_name.camelize %></h1>
<table>
  <thead>
    <tr>
      <% (@resource_class.accessible_attributes - [""]).each do |attr| %>
        <th><%= attr %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @resources.each do |resource| %>
      <tr>
        <% (@resource_class.accessible_attributes - [""]).each do |attr| %>
          <td><%= link_to resource.send(attr), [:admin, resource] %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
<a href="<%= new_admin_user_path %>">New</a>